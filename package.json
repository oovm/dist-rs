{
    "private": true,
    "scripts": {
        "build": "cargo build --release",
        "test": "cargo test --release",
        "fmt": "cargo fmt --all",
        "push": "wee test && git push",
        "reset": "git reset Head~ --soft",
        "u": "cargo upgrade --workspace",
        "patch": "cargo release patch --workspace --no-publish --no-tags --execute",
        "minor": "cargo release patch --workspace --no-publish --no-tags --execute",
        "patch-r": "cargo release patch --workspace --no-tags --unpublished",
        "minor-r": "cargo release minor --workspace --no-tags --unpublished"
    }
}
